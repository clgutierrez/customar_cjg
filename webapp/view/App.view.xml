<mvc:view controllerName="dttgroup.SAPUI5.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core">
	<Shell>
		<App class="myCustomCss">
			<pages>
				<Page title="{i18n>homePageTitle}">
					<headerContent>
						<Button id="helloDialogHeader" icon="sap-icon://world" press=".onOpenHeader" class="sapUiSmallMarginEnd"/>
					</headerContent>
					<content>
						<mvc:XMLView viewName="dttgroup.SAPUI5.view.HelloPanel"/>
						<mvc:XMLView viewName="dttgroup.SAPUI5.view.CustomerList"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:view>